{
  "name": "virtual-next-routes",
  "version": "0.0.29",
  "description": "virtual-next-routes",
  "keywords": [],
  "homepage": "https://github.com/nrjdalal/virtual-next-routes#readme",
  "bugs": "https://github.com/nrjdalal/virtual-next-routes/issues",
  "repository": "nrjdalal/virtual-next-routes",
  "funding": "https://github.com/sponsors/nrjdalal",
  "license": "MIT",
  "author": {
    "name": "Neeraj Dalal",
    "email": "admin@nrjdalal.com",
    "url": "https://nrjdalal.com"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./dist/bin/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "bin": "tsup && node dist/bin/index.js --no-watch",
    "build": "tsup",
    "dev": "tsup --watch",
    "prepare": "npx simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx commitlint --edit $1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown",
    "package.json": "sort-package-json"
  },
  "prettier": {
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports"
    ],
    "semi": false
  },
  "dependencies": {
    "chokidar": "^5.0.0",
    "tinyglobby": "^0.2.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@tanstack/virtual-file-routes": "^1.141.0",
    "@types/node": "^24.10.3",
    "@types/react": "^19.2.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "simple-git-hooks": "^2.13.1",
    "sort-package-json": "^3.5.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.7"
  },
  "peerDependencies": {
    "@tanstack/virtual-file-routes": ">=1.0.0",
    "vite": ">=6.0.0"
  }
}
